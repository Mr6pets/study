/* 导入模板的样式 */
@import '../movieone/movieone.wxss';
.movielist{
  box-sizing: border-box;
  padding: 0 20rpx;
  overflow: hidden;
}
.th{
  height: 50rpx;
  font-size: 40rpx;
}
.th .title{
  float: left;
}
.th navigator{
  float: right;
}
.wrapper{
  width: 100%;
  display: flex;
}
.wrapper view{
  flex: 1;
}